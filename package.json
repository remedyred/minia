{
	"name": "@remedyred/minia",
	"description": "Teeny tiny composition stores",
	"version": "0.0.5",
	"license": "UNLICENSED",
	"exports": {
		".": {
			"require": "./lib/index.js",
			"import": "./src/index.js"
		},
		"./*": {
			"require": "./lib/*.js",
			"import": "./src/*.js"
		}
	},
	"main": "./lib/index.js",
	"module": "./src/index.js",
	"types": "./types/index.d.ts",
	"files": [
		"lib",
		"types",
		"src"
	],
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"build": "babler src lib -c",
		"clean": "shx rm -rf lib",
		"types": "tsc",
		"index": "indexer src -c",
		"test": "jest",
		"start": "node ./_tests_/out.examples.js",
		"version": "yarn clean && yarn build -m",
		"watch": "yarn build -w"
	},
	"dependencies": {
		"@remedyred/out": "^1.0.35",
		"@remedyred/utilities": "1.0.20",
		"localforage": "^1.10.0",
		"mitt": "^3.0.0",
		"vue-demi": "^0.12.5"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@babel/preset-env": "^7.16.11",
		"@remedyred/babler": "^1.3.83",
		"babel-jest": "^27.4.6",
		"babel-plugin-add-module-exports": "^1.0.4",
		"jest": "^27.4.7",
		"node-ray": "^1.18.0",
		"ts-migrate": "^0.1.28",
		"typescript": "^4.6.3"
	}
}
